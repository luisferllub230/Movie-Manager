
<div class="container text-center">
    <h1>{{message}}</h1>
</div>

<div class="row ">
    {{#each movieDataContainer as |i key|}} 
        <div class="col-12 col-md-6 col-lg-4">
            <div class=" card box bg-white text-dark box-size">
                <img src="{{i.urlImage}}" class="card-img-top" alt="{{i.description}}">
                <div class="card-img-overlay img-fluid">
                    <h5 class="card-title">{{i.title}}</h5>
                </div>
            </div>
            {{#if check}}
            <p class="bg-warning txt-white">Active</p>
            {{/if}}
        </div>
    {{/each}}
</div>

